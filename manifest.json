{
  "manifest_version": 2,

  "name": "DarkMode",
  "description": "A dark theme for popular websites.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "assets/img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
],
"content_scripts": [{
    "css": ["assets/css/style.css"],
    "js": ["assets/js/content.js"],
    "matches": ["https://medium.com/*"]
  }]
}
